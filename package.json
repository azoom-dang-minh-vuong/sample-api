{
  "name": "sample-api",
  "version": "2.0.1",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "dev": "env $(grep -v '^#' .env) tsx watch ./src/index.ts",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "start": "env $(grep -v '^#' .env) TSX_TSCONFIG_PATH=tsconfig.build.json node --import tsx/esm ./dist/index.js",
    "staging:clean-tags": "git fetch --tags && git push origin --delete $(git tag -l 'staging-*') || true && git fetch origin --prune 'refs/tags/*:refs/tags/*'",
    "staging:deploy": "yarn staging:clean-tags && TAG=`echo staging-$(date +%Y%m%d-%H%M%S)` && git tag $TAG && git push origin $TAG",
    "test:run": "env $(grep -v '^#' .env.test) vitest",
    "test": "env $(grep -v '^#' .env.test) vitest --watch=false"
  },
  "dependencies": {
    "@azoom/nnn-router": "2.0.0-beta.0.9",
    "@prisma/client": "^5.11.0",
    "cors": "^2.8.5",
    "date-fns": "^3.4.0",
    "express": "^4.18.3",
    "express-openapi-validator": "^5.1.6",
    "got": "^12.0.0",
    "lodash": "^4.17.21",
    "prisma": "^5.11.0",
    "statuses": "^2.0.1",
    "tsx": "^4.7.1"
  },
  "devDependencies": {
    "@azoom/test-http-server": "^1.0.2",
    "@faker-js/faker": "^8.4.1",
    "@quramy/prisma-fabbrica": "^2.1.1",
    "@types/cors": "^2.8.17",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.3.1"
  },
  "volta": {
    "node": "20.11.1",
    "yarn": "1.22.21"
  },
  "engines": {
    "node": ">= 20.6.0"
  }
}
